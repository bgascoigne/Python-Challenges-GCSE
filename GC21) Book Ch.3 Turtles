Question 7:

import turtle

def createWindow():
    window = turtle.Screen()
    window.bgcolor("lightgreen")
    return window

def createTurtle():
    ttl = turtle.Turtle()
    ttl.shape("turtle")
    ttl.color("blue")
    return ttl

def movement():
    angleTurns = [160, -43, 270, -97, -43, 200, -940, 17, -86]
    for angle in angleTurns:
        ttl.left(angle)
        ttl.forward(100)

window = createWindow()
ttl = createTurtle()
movement()

window.mainloop()

------------------------------------------------------------------------------------
Question 8:

import turtle

def createWindow():
    window = turtle.Screen()
    window.bgcolor("lightgreen")
    return window

def createTurtle():
    ttl = turtle.Turtle()
    ttl.shape("turtle")
    ttl.color("blue")
    return ttl

def movement():
    angleTurns = [160, -43, 270, -97, -43, 200, -940, 17, -86]
    endAngle = 0
    for angle in angleTurns:
        ttl.left(angle)
        ttl.forward(100)
        endAngle += angle
    return endAngle

window = createWindow()
ttl = createTurtle()
endAngle = movement()
print(360 % endAngle)

window.mainloop()

