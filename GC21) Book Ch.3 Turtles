Question 5:

numberSequence = [12, 10, 32, 3, 66, 17, 42, 99, 20]

total = 0
for number in numberSequence:
    print(number)
    print(number ** 2)
    total += number
    print(total)
    baseNumber = 1
    for x in numberSequence:
        baseNumber = baseNumber * x
    print(baseNumber)

------------------------------------------------------------------------------------
Question 6:

import turtle

def createWindow():
    window = turtle.Screen()
    window.bgcolor("lightgreen")
    return window

def createTurtle():
    ttl = turtle.Turtle()
    ttl.shape("turtle")
    ttl.color("blue")
    return ttl

def drawObjects():
    for n in range(3):          #Triangle
        ttl.forward(50)
        ttl.left(120)
    ttl.penup()
    ttl.forward(100)
    ttl.pendown()
    for n in range(4):          #Square
        ttl.forward(50)
        ttl.left(90)
    ttl.penup()
    ttl.forward(100)
    ttl.pendown()
    for n in range(6):          #Hexagon 
        ttl.forward(50)
        ttl.left(60)
    ttl.penup()
    ttl.forward(120)
    ttl.pendown()
    for n in range(8):          #Octagon
        ttl.forward(50)
        ttl.left(45)

window = createWindow()
ttl = createTurtle()
drawObjects()

window.mainloop()

